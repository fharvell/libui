#!/usr/bin/env libui
#####
#
# test_Info
#
#####

test_Info () {
  LibuiGetDisplayTestValues
  TERMINAL=true LibuiPerformTest 'Info "Test 1."; Info "Test 2."'
  if ${LIBUI_PLAIN}
  then
    LibuiValidateTest ${?} 0
  else
    LibuiValidateTest ${?} 0 "${TJBL}${TInfo}Test 1.${T}${TCEL}${TJBL}${TJBL}${TInfo}Test 2.${T}${TCEL}${TJBL}"
  fi
  return ${?}
}

return 0
