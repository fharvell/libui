#!/usr/bin/env libui
#####
#
# test_Y_No_Term_Verify
#
#####

test_Y_No_Term_Verify () {
  local t="${TERMINAL}"
  TERMINAL=false
  LibuiPerformTest 'Verify "Test verify?"'
  local tv=${?}
  TERMINAL="${t}"
  LibuiValidateTest "${tv}" 0 'Test verify? (y/n) [yes] yes'
  return ${?}
}

# test setup
_Test_opt+='-X Y '

return 0
