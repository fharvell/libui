#!/usr/bin/env libui
#####
#
# test_C_Confirm_No_Term_Verify-C
#
#####

test_C_Confirm_No_Term_Verify-C () {
  local t="${TERMINAL}"
  TERMINAL=false
  LibuiPerformTest 'Verify -C "Test verify?"'
  local tv=${?}
  TERMINAL="${t}"
  LibuiValidateTest "${tv}" 0 'Test verify? (y/n) [yes] yes'
  return ${?}
}

# test setup
_Test_opt+='-X C '

return 0
