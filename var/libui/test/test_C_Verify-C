#!/usr/bin/env libui
#####
#
# test_C_Verify-C
#
#####

test_C_Verify-C () {
  local t="${TERMINAL}"
  TERMINAL=true
  LibuiPerformTest 'Verify -C "Test verify?" <<< "yes"'
  local tv=${?}
  TERMINAL="${t}"
  LibuiValidateTest "${tv}" 0 'Test verify? (y/n) [yes] '
  return ${?}
}

# test setup
_Test_opt+='-X C '

return 0
