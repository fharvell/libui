#!/usr/bin/env libui
#####
#
# test_Tell-i
#
#####

test_Tell-i () {
  LibuiGetDisplayTestValues
  local t="${TERMINAL}"
  TERMINAL=true
  _Terminal
  TERMINAL="${t}"
  LibuiPerformTest 'Tell -N "Test 1. "; Tell -i "Test 2."'
  LibuiValidateTest ${?} 0 "${TJBL}${TTell}Test 1. ${T}${TCEL}${TTell}Test 2.${T}${TCEL}"
  return ${?}
}

return 0
